{
  "type": "commonjs",
  "scripts": {
    "dev": "NODE_ENV=dev vite",
    "build": "nest build",
    "start": "nest build && sls offline",
    "deploy": "nest build && sls deploy",
    "deploy:local": "nest build && sls deploy --stage local",
    "deploy:prod": "nest build && sls deploy --stage prod",
    "test": "vitest --run",
    "docs": "compodoc -p tsconfig.doc.json -s",
    "format": "prettier src --write --ignore-unknown",
    "lint": "eslint src --ext .ts",
    "prepare": "is-ci ||husky install",
    "commit": "git add . && git commit -m ''"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.350.0",
    "@aws-sdk/client-sns": "^3.350.0",
    "@fastify/aws-lambda": "^3.2.0",
    "@fastify/helmet": "^10.1.1",
    "@fastify/static": "^6.10.2",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.4.2",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.2",
    "@nestjs/platform-fastify": "^9.4.2",
    "@nestjs/swagger": "^6.3.0",
    "aws-jwt-verify": "^4.0.0",
    "aws-lambda": "^1.0.7",
    "cache-manager": "^5.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dynamoose": "^3.2.0",
    "fastify": "^4.18.0",
    "nestjs-cls": "^3.4.0",
    "nestjs-dynamoose": "^0.5.4",
    "rxjs": "^7.8.1",
    "swagger-ui-dist": "^4.19.0",
    "type-fest": "^3.11.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    "@nestjs/testing": "^9.4.2",
    "@serverless/typescript": "^3.30.1",
    "@swc/core": "^1.3.62",
    "@tsconfig/node-lts": "^18.12.2",
    "@types/aws-lambda": "^8.10.117",
    "@types/node": "^20.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "commitizen": "^4.3.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "serverless": "^3.32.2",
    "serverless-domain-manager": "^7.0.4",
    "serverless-esbuild": "^1.45.1",
    "serverless-localstack": "^1.1.1",
    "serverless-offline": "^12.0.4",
    "serverless-prune-plugin": "^2.0.2",
    "ts-node": "^10.9.1",
    "vite": "^4.3.9",
    "vite-plugin-node": "^3.0.2",
    "vitest": "^0.32.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "bash -c tsc --noEmit",
      "vitest related --run"
    ],
    "*.!ts": "prettier --write --ignore-unknown"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}